<odoo>

    <record id="view_warehouse_form" model="ir.ui.view">
        <field name="name">Warehouse form - cycle count extension</field>
        <field name="model">stock.warehouse</field>
        <field name="inherit_id" ref="stock.view_warehouse"/>
        <field name="arch" type="xml">
            <notebook position="before">
                <button name="action_compute_cycle_count_rules" type="object"
                    string="compute cycle count rule" class="oe_highlight"/>
                <group>
                    <field name="test_field"/>
                    <field name="cycle_count_rule_ids">Cycle count
                        rules</field>
                </group>
            </notebook>
        </field>
    </record>

</odoo>
